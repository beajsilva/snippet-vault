generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Snippet {
  id String  @id @default(cuid())
  userId String
  title String
  code String
  language String
  creationDate DateTime @default(now())
  isPublic Boolean @default(false)
}